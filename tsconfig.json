{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "paths": {
      "~": [
        "./"
      ],
      "~/*": [
        "./*"
      ],
      "@": [
        "./"
      ],
      "@/*": [
        "./*"
      ],
      "~~": [
        "./"
      ],
      "~~/*": [
        "./*"
      ],
      "@@": [
        "./"
      ],
      "@@/*": [
        "./*"
      ],
      "assets": [
        "./assets"
      ],
      "assets/*": [
        "./assets/*"
      ],
      "public": [
        "./public"
      ],
      "public/*": [
        "./public/*"
      ],
      "/": [
        "./"
      ],
      "/*": [
        "./*"
      ],
      "/_nuxt/*": [
        "./*"
      ],
      "#components": [
        "./components"
      ],
      "#components/*": [
        "./components/*"
      ],
      "#composables": [
        "./composables"
      ],
      "#composables/*": [
        "./composables/*"
      ],
      "#helpers": [
        "./helpers"
      ],
      "#helpers/*": [
        "./helpers/*"
      ],
      "#root": [
        "."
      ],
      "#root/*": [
        "./*"
      ],
      "#shared": [
        "./lib/shared"
      ],
      "#shared/*": [
        "./lib/shared/*"
      ],
      "#test": [
        "./test"
      ],
      "#test/*": [
        "./test/*"
      ],
      "#test-utils": [
        "./test/utils"
      ],
      "#test-utils/*": [
        "./test/utils/*"
      ],
      "#vue-router": [
        "./.nuxt/vue-router-stub"
      ],
      "#imports": [
        "./.nuxt/imports"
      ],
      "#build": [
        "./.nuxt"
      ],
      "#build/*": [
        "./.nuxt/*"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "useDefineForClassFields": true
  },
  "exclude": [
    "**/node_modules",
    "./.output",
    "./dev-dist",
    "./dist",
    "./src-tauri/gen/schemas",
    "./src-tauri/target"
  ]
}
